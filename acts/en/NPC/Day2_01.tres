metadata/_custom_type_script = "uid://bw5nvw6g1w033"

[sub_resource type="Resource" id="Line_02"]
script = ExtResource("1_dialogue_line")
speaker = 1
text = "Hello. You here to ski too?"
metadata/_custom_type_script = "uid://bw5nvw6g1w033"

[sub_resource type="Resource" id="Line_03"]
script = ExtResource("1_dialogue_line")
text = "Yes. It’s a fine slope, but not many people know about it. I’m surprised to meet someone here."
metadata/_custom_type_script = "uid://bw5nvw6g1w033"

[sub_resource type="Resource" id="Line_04"]
script = ExtResource("1_dialogue_line")
speaker = 1
text = "Yeah. I’ve been staying nearby. Someone told me there was a decent ski field around here. I took the risk of getting lost just to find it."
metadata/_custom_type_script = "uid://bw5nvw6g1w033"

[sub_resource type="Resource" id="Line_05"]
script = ExtResource("1_dialogue_line")
text = "The slope’s right up there."
metadata/_custom_type_script = "uid://bw5nvw6g1w033"

[sub_resource type="Resource" id="Line_06"]
script = ExtResource("1_dialogue_line")
speaker = 1
text = "That’s good to hear. I can’t wait to try it."
metadata/_custom_type_script = "uid://bw5nvw6g1w033"

