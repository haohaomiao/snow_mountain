[sub_resource type="Resource" id="Line_01"]
script = ExtResource("1_dialogue_line")
portrait = 2
text = "I used to ski. Then I got injured and had to retire. People cried when they saw me back then."
metadata/_custom_type_script = "uid://bw5nvw6g1w033"

[sub_resource type="Resource" id="Line_02"]
script = ExtResource("1_dialogue_line")
speaker = 1
portrait = 2
text = "Back then? Why are you crying now?"
metadata/_custom_type_script = "uid://bw5nvw6g1w033"

[sub_resource type="Resource" id="Line_03"]
script = ExtResource("1_dialogue_line")
portrait = 1
text = "Never mind. Seeing you reminds me of when I first started skiing—happy, eyes bright, shining."
metadata/_custom_type_script = "uid://bw5nvw6g1w033"

[sub_resource type="Resource" id="Line_04"]
script = ExtResource("1_dialogue_line")
speaker = 1
portrait = 0
text = "(Nods) Looks like I need to practice harder."
metadata/_custom_type_script = "uid://bw5nvw6g1w033"

[sub_resource type="Resource" id="Line_05"]
script = ExtResource("1_dialogue_line")
speaker = 1
portrait = 0
text = "I’m still not good enough. No wonder it makes you sad to watch. That’s only natural."
metadata/_custom_type_script = "uid://bw5nvw6g1w033"

